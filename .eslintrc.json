{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
      "react",
      "babel"
  ],
  "parserOptions": {
      "ecmaVersion": 6,
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "rules": {
    "prefer-arrow-callback": 1,
    "max-len": [2, 120, 2]
  },
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "es6":true
  }
}
